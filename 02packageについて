library() #インストール済みのパッケージの表示
search()  #読み込み済みのパッケージの表示

#一括でインストールしたい場合は、以下の変換をすると簡単
#「library(」を「install.packages("」
#「)」を「")」


library(compiler) #自作関数などをコンパイルするインタフェース
library(parallel) #並列計算処理をサポート
library(foreign)  #Minitab,S,SAS,SPSS,Stana,Systat,dBaseなどの読み込み
library(grid) #Gridグラフィクス
library(kernSmooth) #カーネルスムージング関数
library(lattice)  #グラフィックス格子
library(mgcv) #GAMs with GCV（平滑化の推定など）
library(nlme) #混合効果モデル
library(rcompgen) #コマンドライン完成ジェネレーター
library(rpart)  #帰納的な分割（決定木）
library(spatial)  #クリギングと点配置解析関数
library(splines)  #回帰スプライン関数と関数
library(stats4) #S4クラスに使用される統計的関数
library(survival) #生存分析
library(tcltk)  #Tcl/Tkインタフェース
library(tools)  #パッケージ開発ツール

library(tidyverse)  #パッケージを集約したパッケージ
#library(readr)  ##読み込みに関するパッケージ
library(data.table) ##tidyverseに含まれていないけれど読み込みに強いパッケージ

#library(ggplot2) ##可視化に関するパッケージ

##可視化の視覚変更
library(RColorBrewer) ##可視化の際に豊富な色を使用するためのパッケージ
library(viridis)  ##Pythonのmatplotlibライブラリと同じ色空間を使用可能にする
library(ggthemes) ##色覚多様性に配慮したカラーパレットの適用
##GUIベースで可視化
library(ggplotgui)  ##ggplot_shiny()でデータフレームを指定してマウス操作で可視化できる
library(ggpubr) ##適当に調べて...（僕自身も理解してない）(cfパッケージ. gridExtra,cowplot)

library(gapminder)  ##折れ線グラフ作成
library(psych)  ##心理学で用いられる統計処理方法のパッケージ
library(dplyr)  ##データ整形に関するパッケージ
library(tidyr)  ##tidy dataを作成する際に活用するパッケージ（Rにとって扱いやすくするため　）

#ネットワークに関するパッケージ
library(rvest)      ##スクレイピングに関するパッケージ（別途読む込み必要）
library(RSelenium)  ##スクレイピングに関するパッケージ（別途調べて...）
library(rtweet)     ##twitterAPIに関するパッケージ


#回帰等の分析に関するパッケージ

library(corrplot) #算出した相関係数を可視化(色彩で表現)
library(coefplot) #相関係数の結果をグラフ化(点プロット)
library(jtool)  ##重回帰分析の結果を視覚的に表示する
library(MASS) #多変量データ解析に使う
library(boot) #Bootstrap関数
library(class)  #分類関数
library(cluster)  #クラスター分析
library(codetools)  #Rコードの分析ツール

#データの読み込みに関するパッケージ
library(readxl) #Excelファイルの読み込み
#read_excel(xlsx/data.xlsx, sheet=1)
library(haven)  #SAS,SPSS,STATAファイルの読み込み
#read_sas
#read_sav ##SPSS
#read_dta ##STATA

library(stargazer)  #記述統計の表の作成ができるパッケージ

library(devtools) #install_githubを含んだパッケージ

#おすすめ関数
library(data.table) ##tidyverseに含まれていないけれど読み込みに強いパッケージ
library(tidyverse)  #パッケージを集約したパッケージ
library(RColorBrewer) ##可視化の際に豊富な色を使用するためのパッケージ

library(devtools) #install_githubを含んだパッケージ


#----------------------------------------------------------------------
library(Rcmdr) #Rコマンダーは、直感的に分析するのに最適

